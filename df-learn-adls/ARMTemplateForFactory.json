{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "df-learn-adls"
		},
		"AzureDataLakeStorage_stornwadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://stornwadls.dfs.core.windows.net"
		},
		"AzureFileStorage_Northwind_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\stornwrawdata.file.core.windows.net\\incomingdata"
		},
		"AzureFileStorage_Northwind_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Azure\\stornwrawdata"
		},
		"AzureKeyVault_Learning_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-learning.vault.azure.net"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/nwCustomers')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureFileStorage_Northwind",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "files"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureFileStorageLocation",
						"fileName": "NTCL_Customer.csv"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureFileStorage_Northwind')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/nwProduct')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureFileStorage_Northwind",
					"type": "LinkedServiceReference"
				},
				"folder": {
					"name": "files"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureFileStorageLocation",
						"fileName": "NTCL_Product.csv"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"quoteChar": "\""
				},
				"schema": [
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					},
					{
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureFileStorage_Northwind')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage_stornwadls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage_stornwadls_properties_typeProperties_url')]",
					"accountKey": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "AzureKeyVault_Learning",
							"type": "LinkedServiceReference"
						},
						"secretName": "stornwadls-key"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureKeyVault_Learning')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureFileStorage_Northwind')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "files used earlier",
				"annotations": [],
				"type": "AzureFileStorage",
				"typeProperties": {
					"host": "[parameters('AzureFileStorage_Northwind_properties_typeProperties_host')]",
					"userId": "[parameters('AzureFileStorage_Northwind_properties_typeProperties_userId')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "AzureKeyVault_Learning",
							"type": "LinkedServiceReference"
						},
						"secretName": "stornwrawdata-key"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureKeyVault_Learning')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureKeyVault_Learning')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "The learning key vault",
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('AzureKeyVault_Learning_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		}
	]
}